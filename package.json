{
  "name": "gitbook-plugin-contributors-each-page",
  "version": "1.0.0",
  "description": "A gitbook plugins that adds the list of contributors for each page",
  "engines": {
    "gitbook": ">1.x.x"
  },
  "gitbook": {
    "properties": {
      "owner": {
        "type": "string",
        "description": "The owner of the repository"
      },
      "repository": {
        "type": "string",
        "description": "The repository name"
      },
      "title": {
        "type": "string",
        "description": "Title of the section"
      },
      "excluded": {
        "type": "array",
        "description": "Path of file where it should not be added"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sandoche/Gitbook-plugin-contributors-each-page.git"
  },
  "keywords": [
    "github",
    "authors",
    "contributors",
    "file",
    "specific",
    "contributions",
    "repository",
    "gibook"
  ],
  "author": "Sandoche ADITTANE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sandoche/Gitbook-plugin-contributors-each-page/issues"
  },
  "homepage": "https://github.com/sandoche/Gitbook-plugin-contributors-each-page",
  "dependencies": {
    "dotenv": "^6.2.0",
    "github-contributors-file": "^1.0.2",
    "node-fetch": "^2.3.0"
  }
}
